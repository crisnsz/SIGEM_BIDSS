@model IEnumerable<SIGEM_BIDSS.Models.tbSolicitudReembolsoGastosDetalle>


@using SIGEM_BIDSS.Models;


<div class="card card-primary card-outline">
    <div class="card-header d-flex p-0 ui-sortable-handle">
        <h3 class="card-title p-3">
            <i class="fa fa-info  mr-1"></i>
            Detalle Solicitud Reembolso
        </h3>

    </div>
    <div class="card-body">
        <table class="table table-bordered table-hover" id="dataTables">
            <thead>
                <tr>
                    <td>Fecha Gasto</td>
                    <td>Gasto</td>
                    <td>Monto Gasto</td>
                    <td>Concepto</td>
                    <td>Archivo</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.ReemgaDet_FechaGasto)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tbTipoViatico.tpv_Descripcion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ReemgaDet_MontoGasto)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ReemgaDet_Concepto)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ReemgaDet_Archivo)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>



