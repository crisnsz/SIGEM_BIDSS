@model IEnumerable<SIGEM_BIDSS.Models.tbLiquidacionAnticipoViatico>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<input type="hidden" id="vSwal" value="@TempData["swalfunction"]" />
<link href="~/Content/template/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
<link href="~/Content/template/plugins/datatables/dataTables.bootstrap4.css" rel="stylesheet" />

<div class="card card-primary card-outline">
    <div class="card-header d-flex p-0 ui-sortable-handle">
        <h3 class="card-title p-3">
            <i class="fa fa-list mr-1"></i>
            Listado de Solicitudes de Anticipo de Viáticos
        </h3>
        <ul class="nav nav-pills ml-auto p-2"></ul>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered table-hover" id="dataTable">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Lianvi_Correlativo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Lianvi_FechaLiquida)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Lianvi_FechaInicioViaje)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Lianvi_FechaFinViaje)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Lianvi_Comentario)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Lianvi_RazonRechazo)
                        </th>

                        <th>Acciones</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Lianvi_Correlativo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Lianvi_FechaLiquida)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Lianvi_FechaInicioViaje)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Lianvi_FechaFinViaje)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Lianvi_Comentario)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Lianvi_RazonRechazo)
                        </td>

                        <td>

                        <td>
                            @Html.ActionLink("Detalles", "Details", new { id = item.Lianvi_Id }, new { @class = "btn btn-default btn-xs", @style = "width: 57px;" })
                        </td>
                       
                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
@section Scripts {



    <!-- DataTables -->
    <script src="~/Content/template/plugins/datatables/jquery.dataTables.js"></script>
    <script src="~/Content/template/plugins/datatables/dataTables.bootstrap4.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/template/dist/js/adminlte.min.js"></script>

    <script src="~/Scripts/app/DataTables.js"></script>

}